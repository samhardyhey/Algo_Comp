--set tempo, 120 bpm
cps (120/60/2)

--bird sound sequences
d1 $ sound "[birds:5 birds:4 birds:3] [birds:6]"

--granular synthesis
d1 $ rev $ chop 16 $ sound "bd ~ sn bd ~ [~ bd]/2 [sn [~ bd]/2] ~"

--granular chop, slowdown and reverse of break beat
d1 $ rev $ slow 4 $ chop 16 $ sound "breaks125:1"

--bottle cacophony
d1 $ every 4 (rev) (sound "[bottle:2 bottle:3 bottle:6] [bottle:5 bottle:6 bottle:7 bottle:8 bottle:6]/2")
d2 $ sound "[bottle:2(5,8), bottle:1(1,3) bottle:6(2,5)]*2"

hush
